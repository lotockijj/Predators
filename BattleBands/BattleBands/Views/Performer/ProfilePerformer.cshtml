@model BattleBands.Models.ApplicationPerformer

<h1>@Model.PerformerName</h1><br />

<label>Genre/style:</label><br />
<p>@Model.PerformerGenre</p><br />
<label>Emali:</label><br />
<p>@Model.PerformerEmail</p><br />
<label>Country:</label><br />
<p>@Model.PerformerCountry</p><br />
<label>Phone:</label><br />
<p>@Model.PerformerPhone</p><br />
<label>Description:</label><br />
<p>@Model.PerformerDescription</p><br />
<p><a asp-action="AddVideo" asp-route-id="@Model.PerformerId" class="btn btn-default">Add video</a></p>
<p>
    <a asp-controller="Performer" asp-action="UpdatePerformer" asp-route-id="@Model.PerformerId" class="btn btn-default">Edit performer</a>
</p>
<p>
    <a asp-action="GetPerformerVideos" asp-route-id="@Model.PerformerId"class="btn btn-default">"@Model.PerformerName" Videos</a>
</p>

<a asp-action="Index" class="btn btn-default"> Back to All performers</a>


